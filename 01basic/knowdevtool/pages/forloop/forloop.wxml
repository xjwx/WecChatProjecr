<!--pages/forloop/forloop.wxml-->
<view wx:for="{{books}}">
  {{item}}/{{index}}
</view>
<view class='row' wx:for="{{[1,2,3,4,5,6,7,8,9]}}" wx:for-item ="row">
  <view class='col' wx:for="{{[1,2,3,4,5,6,7,8,9]}}" wx:for-item="col" wx:if="{{col<=row}}">
      {{row}}*{{col}}={{row*col}}
  </view>
</view>